.recommendation {
    width: 100%;
    display: flex;
    flex-direction: column;
    background-color: #f5f5f5;
    padding-bottom: 14rpx;
}
.recommendation-header {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    height: 64rpx;
    padding:0 20rpx;
    line-height: 64rpx;
}
.recommendation-header .title {
    font-size: 30rpx;
    color: #394675;
}
.recommendation-header .more {
    font-size: 26rpx;
    color: #787e85;
}
