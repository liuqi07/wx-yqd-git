/* index.wxss */
@import "condition/condition-tpl.wxss";
@import "invest/invest-tpl.wxss";
@import "../common/dialog-tpl/dialog-tpl.wxss";

.dialog {
    width: 100%;
    height: 100%;
    position: fixed;
    z-index: 999;
}

.container {
    display: flex;
    flex-direction: column;
    background-color: #f5f5f5;
}
swiper {
    width: 100%;
    height: 290rpx;
}
swiper image {
    width: 100%;
    height: 290rpx;
}

.theNoviceExclusive {
    width: 100%;
    height: 178rpx;
    background: url('data:image/png;base64,');
    background-size: cover;
    display: flex;
    flex-direction: column;
    align-items: center;
    padding-top: 22rpx;
}
.theNoviceExclusive text{
    font-size: 26rpx;
    line-height: 48rpx;
    color: #394675;
}
.theNoviceExclusive .toGet {
    width: 360rpx;
    height: 60rpx;
    text-align: center;
    line-height: 60rpx;
    border-radius: 30rpx;
    background: -webkit-linear-gradient(left , rgb(14, 109, 183) 7% , rgb(62, 153, 230) 83%);
}
.theNoviceExclusive .toGet text {
    font-size: 28rpx;
    color: #fff;
}

.greenhand {
    height: 260rpx;
    display: flex;
    margin-top: 18rpx;
    background-color: #fff;
    padding: 0 20rpx 0 40rpx;
}
.greenhand .greenhand-l {
    width: 330rpx;
    display: flex;
    flex-direction: column;
}
.greenhand-l .title{
    font-size: 50rpx;
    color: #ff6767;
    line-height: 108rpx;
    font-weight: 800;
}
.greenhand-l .txt {
    width: 230rpx;
    height: 154rpx;
    background: url('data:image/png;base64,') no-repeat;
    background-size: 166rpx 154rpx;
    font-size: 30rpx;
    color: #6d7490;
}
.greenhand .greenhand-r {
    display: flex;
    flex-direction: column;
}
.greenhand-r .tip {
    width: 360rpx;
    height: 60rpx;
    line-height: 60rpx;
    text-align: right;
    margin-top: 10rpx;
    font-size: 26rpx;
    color: #757b8c;
}
.greenhand-r .tip text:last-of-type {
    color: #ff6767;
}
.greenhand-r .yield {
    height: 76rpx;
    font-size: 38rpx;
    color: #3f99e6;
    text-indent: 10rpx;
    margin-bottom: 20rpx;
}
.greenhand-r .yield text {
    height:76rpx;
    line-height: 76rpx;
    font-size: 76rpx;
}

