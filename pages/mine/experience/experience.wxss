/* experience.wxss */

/* invest-detail.wxss */

page {
  width: 100%;
  height: 100%;
}

.container {
  width: 100%;
  height: 100%;
  background-color: #fff;
}

.intro {
  width: 100%;
  padding-top: 80rpx;
  padding-bottom: 80rpx;
}

.intro .intro-circle {
  margin: 0rpx auto;
  width: 350rpx;
  height: 350rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  border-radius: 175rpx;
  border: 1px solid #52a3e8;
  font-size: 24rpx;
  background: url('data:image/png;base64,') no-repeat center bottom;
  background-size: 100% auto;
  background-position-y: 210rpx;
}

.intro-circle .title {
  margin-top: 58rpx;
  margin-bottom: 38rpx;
  color: #a4a8bb;
}

.intro-circle view {
  /*height:26rpx;*//*margin-top: 48rpx;*/
  font-size: 30rpx;
  color: #3f99e6;
}

.intro-circle .pre {
  font-size: 46rpx;
  font-weight: 900;
}

.intro-circle .txt {
  /*height: 44rpx;*/
  line-height: 46rpx;
  margin-top: 46rpx;
  color: #e4f1fb;
}

.intro-circle {
  color: #e4f1fb;
}

.bar {
  height: 10rpx;
  width: 100%;
  background: #f5f5f5;
}

.ticket {
  display: flex;
  box-sizing: border-box;
  border-top: 20rpx solid #f5f5f5;
  border-bottom: 20rpx solid #f5f5f5;
  flex-direction: column;
  padding-top: 24rpx;
  padding-bottom: 12rpx;
  align-items: center;
}

.moneyTick {
  flex-grow: 1;
  width: 452rpx;
  height: 120rpx;
  background: url(data:image/png;base64,) no-repeat left top;
  background-size: 100% 100%;
  display: flex;
  /*align-self: center;
  flex-direction: column;
  justify-content: center;*/
  align-items: center;
  margin-bottom: 16rpx;
}

.moneyIcon{
    width: 72rpx;
    height: 72rpx;
    margin:0 30rpx;
}
.tickDescribe {
  padding-top: 20rpx;
  line-height: 50rpx;
  font-size: 28rpx;
  padding-left: 30rpx;
}

.money {
  font-size: 36rpx;
  flex-grow: 1;
  color: #394675;
  font-weight: 100;
}
.money text{
    font-weight: 600;
}
.moneyDes {
  /*padding-top: 5rpx;*/
  font-size: 24rpx;
  margin-top: 6rpx;
}
.usedTime{
    display: flex;
    flex-direction: column;
    align-items: center;
    width: 110rpx;
    flex-shrink: 0;
    font-size: 36rpx;
}
.day{
    font-size: 36rpx;
    text-align: center;
    color: #394675;
}
.state{
    font-size: 18rpx;
    color: #fff;
    background: red;
    width: 64rpx;
    height: 24rpx;
    line-height: 24rpx;
    border-radius: 24rpx;
    text-align: center;
    background: -webkit-linear-gradient(left , #4eaefe , #55e8f5 100%);
    margin-top: 6rpx;
}
.noneData {
  text-align: center;
  margin-top: 286rpx;
}

.noneBg {
  width: 208rpx;
  height: 331rpx;
}
.tip{
    margin-top: 43rpx;
    font-size: 34rpx;
    color: #394675;
}