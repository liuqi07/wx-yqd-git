/* invest-detail.wxss */
page {
    width: 100%;
    height: 100%;
}
.container {
    width: 100%;
    height: 100%;
    background-color: #fff;
}

.intro {
    width: 100%;
    padding-top: 80rpx;
    padding-bottom: 80rpx;
}
.intro .intro-circle {
    margin: 0rpx auto;
    width: 350rpx;
    height: 350rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    border-radius: 175rpx;
    border: 1px solid #52a3e8;
    font-size: 24rpx;
    
    background: url('data:image/png;base64,') no-repeat center bottom;
    background-size: 100% auto;
    background-position-y: 210rpx;
}
.intro-circle .title {
    margin-top: 58rpx;
    margin-bottom: 38rpx;
    color: #a4a8bb;
}
.intro-circle view {
    /*height:26rpx;*/
    /*margin-top: 48rpx;*/
    font-size: 30rpx;
    color: #3f99e6;
}
.intro-circle .pre {
    font-size: 46rpx;
    font-weight: 900;
}
.intro-circle .txt {
    /*height: 44rpx;*/
    line-height: 46rpx;
    margin-top: 46rpx;
    color: #e4f1fb;
}
.intro-circle {
    color: #e4f1fb;
}
.bar{
  height: 10rpx;
  width: 100%;
  background: #f5f5f5;
}
.ticket{
  height: 180rpx;
  display: flex;
  box-sizing: border-box;
  padding:30rpx;
  border-top: 10rpx solid #f5f5f5;
   border-bottom: 10rpx solid #f5f5f5;
}
.moneyTick{
  flex-grow: 1;
  background: green;
  margin: 0 10rpx;
}
.tickDescribe{
  padding-top: 20rpx;
  line-height: 50rpx;
  font-size: 28rpx;
  padding-left: 30rpx;

}
