page{
  background: #f5f5f5;
  height: 100%;
}
.contain{
  height: 100%;
}
.account{
  color: #fff;
  background: -webkit-linear-gradient(left , #0d6cb6 , #3f98e5 100%);
}
.head{
  line-height: 64rpx;
  display: flex;
  padding-top: 10rpx;
}

.head text:nth-of-type(1){
  font-size: 40rpx;
  margin: 0rpx 15rpx;
}
.head text:nth-of-type(2){
  font-size: 24rpx;
}
.logo{
  width: 64rpx;
  height:64rpx;
}
.title{
  flex-grow: 1;
  font-size: 30rpx;
  line-height: 64rpx;
}

.btnLogin{
  /*background:gray;*/
  height:54rpx;
  line-height:54rpx;
  font-size:24rpx;
  color:#fff;
  margin:8rpx;
  padding: 0rpx 20rpx;

}
.accountBalance,.accountDetial{
  display: -webkit-flex;
  display: flex;
  -webkit-justify-content:center;
	justify-content:center;
  font-size: 30rpx;
}
.balance{
  position: relative;
}
.iconInfo{
  position: absolute;
  top: 0;
  right: -50rpx;
  width: 42rpx;
  height: 42rpx;
}
.accountBalance{
  flex-direction: column;
  align-items: center;
  padding-top: 40rpx;
}
.accountBalance text{
  line-height:66rpx;
}
.accountMoney{
  font-size: 56rpx;
}
.accountDetial{
  padding: 47rpx 30rpx;
}
.rewardDetial{
  background: #fff;
}
.accountList{
  flex-grow: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
 border-right: 1rpx solid #61a2d7;
}
.brderRight{
  border-right: 1rpx solid #e6e6e6;
}
.accountList:last-child{
  border-right: none;
}
.accountList text{
  line-height: 64rpx;
}
.moneyCount{
  font-size: 36rpx;
}
.moneyDes{
  font-size: 24rpx;
}

/*水平垂直居中*/
.center{
   display: flex;
  flex-direction: column;
  -webkit-justify-content:center;
	justify-content:center;
  align-items: center;
}

/** 修改button默认的点击态样式类**/
.button-hover {
  background-color: red;
}
/** 添加自定义button点击态样式类**/
.other-button-hover {
  background-color: blue;
}
.btnShare{
  width: 90%;
  margin:50rpx  auto;
  height: 100rpx;
  border-radius: 50rpx;
  background: gray;
  line-height: 100rpx;
}

.invite{
  text-align: center;
}
.inviteHead{
  text-align: center;
  font-size: 42rpx;
}
.inviteReward{
  text-align: center;
  font-size: 24rpx;
  color: #757b8c;
  padding-top: 23rpx;
  padding-bottom: 32rpx;
}
.inviteCode{
  background: #b3b3b3;
  font-size: 30rpx;
  color: #fff;
  width: 360rpx;
  height: 60rpx;
  opacity: 0.5;
  line-height: 60rpx;
  text-align: center;
  border-radius: 30rpx;
  margin: 0 auto;
}

.experienceImg{
  width: 68rpx;
  height: 68rpx;
}
.rewardImg{
  width: 76rpx;
  height: 61rpx;
}
.experienceDtl{
  padding-top: 20rpx;
}
.rewardDtl{
  padding-top: 27rpx;
}
.inviteContain{
  background: url(data:image/png;base64,) no-repeat left top;
  background-size: 100% 100%;
  margin-top: 10rpx;
  padding-top: 50rpx;
  padding-bottom: 35rpx;
}
.loginAccount{
  text-align: center;
  height: 486rpx;
}
.logoImg{
  width: 302rpx;
  height:144rpx;
  margin-top: 143rpx;
  margin-bottom: 73rpx;
}
.loginBtn{
  height: 60rpx;
  border-radius: 10rpx;
  line-height: 60rpx;
  color: #fff;
  font-size: 28rpx;
  display:inline-block;
  border:1px solid #fff;
}
button{
  background: none;
}
button::after{
  border: 0rpx;
}