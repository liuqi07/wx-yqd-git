/* reward.wxss */
page{background-color:#edf0f3;}
.nav{width:100%;height:100rpx;display:flex;flex-direction:row;}
.default{line-height:100rpx;text-align:center;flex:1;font-size:28rpx;}
.default text{
  position: relative;
}
.default.active text::before{
  position: absolute;
  content: "";
  width: 100%;
  height: 6rpx;
  background: red;
  bottom: -20rpx;
  left: 0;
  }
.tick{
  width: 652rpx;
  height: 230rpx;
  /*background: red;*/
  margin: 0 auto;
  display: flex;
  background: #fff;
}
.tickL{
  background: url(data:image/png;base64,) no-repeat left top;
  background-size: 100% 100%;
  flex-grow: 1;
}
.lTop{
  height: 120rpx;
  /*background: green;*/
  display: flex;
}
.money{
  width: 120rpx;
  line-height: 120rpx;
  font-size: 24rpx;
  text-align: center;
  color: #fff;
}
.money text{
  font-size: 50rpx;
  padding-left: 10rpx;
}
.moneyRule{
  flex-flow: 1;
  font-size: 26rpx;
  color: #f9f6d7;
}
.redTick{
  width:178rpx;
  height:39rpx;
  background:#980001;
  border-radius: 10rpx;
  line-height: 39rpx;
  text-align: center;
  margin-top: 29rpx;
  margin-bottom: 12rpx;
  color: #f4e7b2;

}
.birth{
  width: 25rpx;
  height: 25rpx;
  padding-top: 3rpx;
}
.lBot{
  font-size: 24rpx;
  padding-left: 15rpx;
  line-height: 38rpx;
  padding-top: 20rpx;
  color: #fff;
}
.tickR{
  width: 252rpx;
  /*background: green;*/
  flex-shrink:0;
  position: relative;
  box-sizing: border-box;
  padding-top: 70rpx;
  
}
.askIcon{
  width: 31rpx;
  height: 31rpx;
  position: absolute;
  top: 9rpx;
  right: 14rpx;
}
.packet{
  width: 190rpx;
  height: 50rpx;
  color: #c5262a;
  font-size:30rpx;
  border: 2px solid #c5262a;
  border-radius: 10rpx;
  text-align: center;
  line-height: 50rpx;
  margin: 0 auto;
}
.packetInfo{
  padding-top: 40rpx;
  font-size: 24rpx;
  text-align: center;
  color: #717171;
}