/* reward.wxss */
page{background-color:#f5f5f5;}
.nav{width:100%;height:100rpx;display:flex;flex-direction:row; background: #fafafa;}
.default{line-height:100rpx;text-align:center;flex:1;font-size:28rpx;}
.default{
  position: relative;
}
.default:first-child{
  border-right: 1rpx solid #cccccc;
}
.default.active{
  color:#348fdb;
}
.default.active:before{
  position: absolute;
  content: "";
  width: 100%;
  height: 4rpx;
  background: #348fdb;
  bottom: 0;
  left: 0;
  color: #348fdb;
}

.title{
  height: 60rpx;
  line-height: 60rpx;
  background: #fff6cf;
  padding: 0 30rpx;
  font-size: 24rpx;
  color: #394675;
  position: relative;
  margin-bottom: 30rpx;
}
.title text{
  padding-left: 40rpx;
}
.titleIcon{
  width: 28rpx;
  height: 28rpx;
  /*padding-top: 16rpx;*/
  position: absolute;
  top: 16rpx;
}

.tick{
  position: relative;
  margin: 20rpx 30rpx;
  background: url(data:image/png;base64,) no-repeat left top;
  background-size: 100% 100%;
  display: flex;
  height: 204rpx;
  box-sizing: border-box;
  padding: 32rpx 0; 
}
.tickL{
  width: 160rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-right: 49rpx;
  justify-content:space-between;
}
.tickL text{
  font-size: 24rpx;
  color: #394675;
}
.birth{
  width: 98rpx;
  height: 98rpx;
}
.tickR{
  flex-grow: 1;
  color: #394675;
  display: flex;
  justify-content:space-between;
  flex-direction: column;
}
.time{
  font-size: 18rpx;
}
.rule{
  font-size: 24rpx;
}
.money{
  font-size: 24rpx;
}
.money text{
  font-size: 46rpx;
}

.used{
  width: 102rpx;
  height: 102rpx;
  position: absolute;
  top: 0;
  right: 0;
}

/*我的奖励*/
